{
  "name": "kronos-interceptor",
  "version": "0.0.0-semantic-release",
  "description": "introspects / modifies requests as they pass between endpoints",
  "keywords": [
    "kronos-interceptor"
  ],
  "scripts": {
    "cover": "nyc npm test",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "prepublish": "rollup --output=dist/index.js -c -- src/index.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-interceptor.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-interceptor/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-interceptor#readme",
  "dependencies": {
    "model-attributes": "^3.0.7"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^2.0.0",
    "kronos-test-interceptor": "^2.1.0",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "semantic-release": "^6.3.6",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.41.5",
    "nyc": "^10.1.2"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.10.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "module": "src/index.js",
  "main": "dist/index.js",
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  }
}