{
  "name": "@kronos-integration/interceptor",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/index.mjs",
  "exports": {
    ".": "./src/index.mjs"
  },
  "description": "introspects / modifies requests as they pass between endpoints",
  "keywords": [
    "interceptor"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/index.mjs --section=API",
    "lint": "documentation lint ./src/index.mjs"
  },
  "dependencies": {
    "model-attributes": "^4.1.3"
  },
  "devDependencies": {
    "@kronos-integration/test-interceptor": "^7.0.3",
    "ava": "^3.9.0",
    "c8": "^7.2.0",
    "documentation": "^13.0.1",
    "semantic-release": "^17.1.1"
  },
  "engines": {
    "node": ">=14.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/interceptor.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/interceptor/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/interceptor#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-kronos-component",
      "arlac77/template-github"
    ]
  }
}
