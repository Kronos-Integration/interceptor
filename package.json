{
  "name": "kronos-interceptor",
  "version": "0.0.0-semantic-release",
  "description": "introspects / modifies requests as they pass between endpoints",
  "keywords": [
    "kronos-interceptor"
  ],
  "scripts": {
    "cover": "npm run build && node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports tests/*_test.js",
    "test": "npm run build && node_modules/.bin/mocha tests/*_test.js && markdown-doctest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "build": "node_modules/rollup/bin/rollup --output=dist/index.js -c -- src/index.js",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js > README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-interceptor.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-interceptor/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-interceptor#readme",
  "dependencies": {
    "model-attributes": "^2.2.13"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "kronos-test-interceptor": "^2.0.5",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "semantic-release": "^6.3.3",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.36.4",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.9.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "module": "src/index.js",
  "main": "dist/index.js"
}